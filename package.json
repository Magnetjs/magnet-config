{
  "name": "magnet-config",
  "version": "1.19.1",
  "description": "Readme",
  "keywords": [
    "config",
    "magnet",
    "magnetjs"
  ],
  "homepage": "https://github.com/Magnetjs/magnet-config#readme",
  "bugs": {
    "url": "https://github.com/Magnetjs/magnet-config/issues"
  },
  "license": "MIT",
  "author": "Kieve Chua <kievechua@gmail.com> (https://github.com/kievechua)",
  "main": "dist/index.js",
  "types": "dist/index.d.js",
  "repository": "magnetjs/magnet-config",
  "scripts": {
    "build": "npm run clean-lib && npm run build:index && npm run copy-package",
    "build:index": "node_modules/.bin/tsc",
    "clean-lib": "node_modules/.bin/rimraf ./dist",
    "copy-package": "cp -rf package.json LICENSE README.md dist",
    "dist": "npm run build && cd dist && npm publish",
    "start": "tsc",
    "test": "ava | tap-spec",
    "watch": "tsc -w",
    "watch-all": "concurrently \"npm run watch\" \"npm run watch-test\"",
    "watch-test": "ava --watch | tap-spec"
  },
  "dependencies": {
    "@types/lodash": "^4.14.52",
    "@types/node": "^7.0.5",
    "bluebird": "^3.4.7",
    "is-promise": "^2.1.0",
    "lodash": "^4.13.1",
    "magnet-core": "^3.7.1",
    "mz": "^2.6.0",
    "require-all": "^2.0.0",
    "typescript": "^2.1.6"
  },
  "devDependencies": {
    "ava": "^0.18.1",
    "concurrently": "^3.3.0",
    "standard": "^8.6.0",
    "tap-spec": "^4.1.1"
  },
  "engines": {
    "node": ">=6.9.5"
  },
  "ava": {
    "failFast": true,
    "tap": true,
    "files": [
      "test/*.{js,jsx}",
      "!test/config/*"
    ],
    "source": [
      "**/*.{js,jsx}",
      "!dist/**/*"
    ]
  }
}
