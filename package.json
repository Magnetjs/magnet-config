{
  "name": "magnet-config",
  "version": "1.6.1",
  "description": "Readme",
  "keywords": [
    "config",
    "magnet",
    "magnetjs"
  ],
  "homepage": "https://github.com/Magnetjs/magnet-config#readme",
  "bugs": {
    "url": "https://github.com/Magnetjs/magnet-config/issues"
  },
  "license": "MIT",
  "author": "Kieve Chua <kievechua@gmail.com> (https://github.com/kievechua)",
  "files": [
    "README",
    "CHANGELOG.md",
    "dist"
  ],
  "main": "dist/index.js",
  "repository": "magnetjs/magnet-config",
  "scripts": {
    "build": "npm run clean && npm run build-index",
    "build-index": "babel src -d dist",
    "clean": "rm -rf dist && mkdir dist",
    "dist": "npm run build && npm publish",
    "start": "babel src --out-dir dist",
    "test": "ava | tap-spec",
    "watch": "nodemon --ignore dist --exec \"babel src\" --out-dir dist",
    "watch-all": "concurrently \"npm run watch\" \"npm run watch-test\"",
    "watch-test": "ava --watch | tap-spec"
  },
  "babel": {
    "ignore": [
      "node_modules",
      "dist",
      "test"
    ],
    "plugins": [
      "fast-async"
    ],
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ]
  },
  "dependencies": {
    "lodash": "^4.13.1",
    "magnet-core": "^3.4.0",
    "require-all": "^2.0.0"
  },
  "devDependencies": {
    "ava": "^0.18.1",
    "babel-cli": "^6.23.0",
    "babel-preset-env": "^1.1.8",
    "concurrently": "^3.3.0",
    "fast-async": "^6.2.0",
    "nodemon": "^1.9.1",
    "standard": "^8.6.0",
    "tap-spec": "^4.1.1"
  },
  "engines": {
    "node": ">=6.9.5"
  },
  "ava": {
    "failFast": true,
    "tap": true,
    "files": [
      "test/*.{js,jsx}",
      "!test/config/*"
    ],
    "source": [
      "**/*.{js,jsx}",
      "!dist/**/*"
    ]
  }
}
